FROM i386/ubuntu:trusty

ENV DEBIAN_FRONTEND=noninteractive

RUN apt-get update \
    && apt-get --quiet --yes --no-install-recommends install \
      software-properties-common \
      ;

RUN apt-add-repository -y ppa:jonathonf/python-2.7

RUN apt-get update \
    && apt-get install -y --no-install-recommends python2.7=2.7.13-2~14.04.york0

CMD python2.7 -c 'print(__import__("timeit").timeit("pow(2, , )", number=10))'
